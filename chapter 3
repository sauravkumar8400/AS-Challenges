EXCERCISE 12
import turtle
wn = turtle.Screen()
wn.bgcolor("lightgreen")
tess = turtle.Turtle()
tess.shape("turtle")
tess.color("blue")
tess.pensize(5)

tess.stamp()

for i in range(12):
  tess.penup()
  tess.forward(90)
  tess.pendown()
  tess.forward(10)
  tess.penup()
  tess.forward(30)
  tess.stamp()
  tess.backward(130)
  tess.left(30)
  
